<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:layout_marginBottom="1dp"
              android:layout_marginTop="1dp"
              android:background="@color/white"
              android:orientation="horizontal">

    <TextView
            android:id="@+id/tvDate"
            style="@style/recordTitle"
            android:layout_width="150dp"
            android:text="日期"
            android:textColor="@color/myTxtLight"/>

    <include layout="@layout/item_line_vertical"/>

    <TextView
            android:id="@+id/tvTime"
            style="@style/recordTitle"
            android:layout_width="62dp"
            android:text="时间"
            android:textColor="@color/myTxtLight"/>

    <include layout="@layout/item_line_vertical"/>

    <TextView
            android:id="@+id/tvTemperature"
            style="@style/recordTitle"
            android:layout_width="62dp"
            android:text="T\n(℃)"
            android:textColor="@color/myTxtLight"/>

    <include layout="@layout/item_line_vertical"/>

    <TextView
            android:id="@+id/tvHR"
            style="@style/recordTitle"
            android:layout_width="75dp"
            android:text="p/HR\n(次/分)"
            android:textColor="@color/myTxtLight"/>

    <include layout="@layout/item_line_vertical"/>

    <TextView
            android:id="@+id/tvR"
            style="@style/recordTitle"
            android:layout_width="75dp"
            android:textColor="@color/myTxtLight"
            android:text="R\n(次/分)"/>

    <include layout="@layout/item_line_vertical"/>

    <TextView
            android:id="@+id/tvBP"
            style="@style/recordTitle"
            android:layout_width="75dp"
            android:text="BP\n(mmHg)"
            android:textColor="@color/myTxtLight"/>

    <include layout="@layout/item_line_vertical"/>

    <TextView
            android:id="@+id/tvSpo2"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:text="SpO₂\n(%)"
            android:textColor="@color/myTxtLight"/>

    <include layout="@layout/item_line_vertical"/>

    <TextView
            android:id="@+id/tvOC_FHt"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:text="胎心\n次/分"
            android:textColor="@color/myTxtLight"/>

    <include layout="@layout/item_line_vertical"/>

    <TextView
            android:id="@+id/tvOC_FM"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:text="自计胎动\n次/小时"
            android:textColor="@color/myTxtLight"/>

    <include layout="@layout/item_line_vertical"/>

    <TextView
            android:id="@+id/tvOC_POTF"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:text="胎位"
            android:textColor="@color/myTxtLight"/>

    <include layout="@layout/item_line_vertical"/>

    <LinearLayout
            android:layout_width="270dp"
            android:layout_height="match_parent">

        <TextView
                android:id="@+id/tvOC_UCS"
                style="@style/recordTitle"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="强度"
                android:textColor="@color/myTxtLight"/>

        <include layout="@layout/item_line_vertical"/>

        <TextView
                android:id="@+id/tvOC_UCI"
                style="@style/recordTitle"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:text="持续(S)/间歇(min)"
                android:textColor="@color/myTxtLight"/>
    </LinearLayout>

    <include layout="@layout/item_line_vertical"/>

    <LinearLayout
            android:layout_width="200dp"
            android:layout_height="match_parent">

        <TextView
                android:id="@+id/tv_OC_CD"
                style="@style/recordTitle"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="扩张(cm)"
                android:textColor="@color/myTxtLight"/>

        <include layout="@layout/item_line_vertical"/>

        <TextView
                android:id="@+id/tvOC_CS"
                style="@style/recordTitle"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="质"
                android:textColor="@color/myTxtLight"/>

    </LinearLayout>

    <include layout="@layout/item_line_vertical"/>

    <TextView
            android:id="@+id/tvOC_SO"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:text="先露高低\n(cm)"
            android:textColor="@color/myTxtLight"/>

    <include layout="@layout/item_line_vertical"/>

    <TextView
            android:id="@+id/OC_Caul"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:text="胎膜"
            android:textColor="@color/myTxtLight"/>

    <include layout="@layout/item_line_vertical"/>

    <TextView
            android:id="@+id/OC_AFI"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:text="羊水性状"
            android:textColor="@color/myTxtLight"/>

    <include layout="@layout/item_line_vertical"/>

    <LinearLayout
            android:layout_width="200dp"
            android:layout_height="match_parent">

        <TextView
                android:id="@+id/In_Project"
                style="@style/recordTitle"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="项目"
                android:textColor="@color/myTxtLight"/>

        <include layout="@layout/item_line_vertical"/>

        <TextView
                android:id="@+id/In_Amount"
                style="@style/recordTitle"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="量"
                android:textColor="@color/myTxtLight"/>

    </LinearLayout>

    <include layout="@layout/item_line_vertical"/>

    <LinearLayout
            android:layout_width="200dp"
            android:layout_height="match_parent">

        <TextView
                android:id="@+id/Out_Project"
                style="@style/recordTitle"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="项目"
                android:textColor="@color/myTxtLight"/>

        <include layout="@layout/item_line_vertical"/>

        <TextView
                android:id="@+id/Out_Amount"
                style="@style/recordTitle"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="量"
                android:textColor="@color/myTxtLight"/>

    </LinearLayout>

    <include layout="@layout/item_line_vertical"/>

    <TextView
            android:id="@+id/Aerophose"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:text="氧气吸入\n(升/分)"
            android:textColor="@color/myTxtLight"/>

    <include layout="@layout/item_line_vertical"/>

    <TextView
            android:id="@+id/KneeJerk"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:text="膝反射"
            android:textColor="@color/myTxtLight"/>

    <include layout="@layout/item_line_vertical"/>

    <TextView
            android:id="@+id/OtherSituation"
            style="@style/recordTitle"
            android:layout_width="300dp"
            android:maxLines="1"
            android:ellipsize="end"
            android:singleLine="true"
            android:gravity="center"
            android:text="特殊情况记录"
            android:textColor="@color/myTxtLight"/>

    <include layout="@layout/item_line_vertical"/>

    <TextView
            android:id="@+id/ExecutiveNurse"
            style="@style/recordTitle"
            android:layout_width="125dp"
            android:text="护士签名"
            android:textColor="@color/myTxtLight"/>

    <include layout="@layout/item_line_vertical"/>

</LinearLayout>