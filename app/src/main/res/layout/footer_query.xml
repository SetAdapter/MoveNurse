<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:background="#ababab">

        <TextView
            android:id="@+id/tvDate"
            style="@style/recordTitle"
            android:layout_width="120dp"
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />

        <TextView
            android:id="@+id/projectName"
            style="@style/recordTitle"
            android:layout_width="200dp"
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />

        <TextView
            android:id="@+id/tvTemperature"
            style="@style/recordTitle"
            android:layout_width="90dp"
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />

        <TextView
            android:id="@+id/total_amount"
            style="@style/recordTitle"
            android:layout_width="90dp"
            android:text="数量"
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />

        <TextView
            android:id="@+id/tvPrice"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />

        <TextView
            android:id="@+id/tvSpecifications"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />

        <TextView
            android:id="@+id/tvValuationTime"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />

        <TextView
            android:id="@+id/tvMainSequence"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />

        <TextView
            android:id="@+id/tvSubSequence"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />

        <TextView
            android:id="@+id/tvBatchNumber"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:text=""
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />

        <TextView
            android:id="@+id/tvRetailPrice"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:text=""
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />
        <TextView
            android:id="@+id/tvBillingDoctor"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />

        <TextView
            android:id="@+id/tvPrescriptionTypes"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />

        <TextView
            android:id="@+id/tvPrescriptionNumber"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />

        <TextView
            android:id="@+id/tvVender"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />

        <TextView
            android:id="@+id/total_fNumber"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:text="金额"
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />

        <TextView
            android:id="@+id/total_feeAmount"
            style="@style/recordTitle"
            android:layout_width="140dp"
            android:text="自费金额"
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />

        <TextView
            android:id="@+id/orderDepName"
            style="@style/recordTitle"
            android:layout_width="110dp"
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />

        <TextView
            android:id="@+id/executiveDepName"
            style="@style/recordTitle"
            android:layout_width="110dp"
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />


        <TextView
            android:id="@+id/patID"
            style="@style/recordTitle"
            android:layout_width="110dp"
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />

        <TextView
            android:id="@+id/costName"
            style="@style/recordTitle"
            android:layout_width="120dp"
            android:text="合计"
            android:textColor="@color/white" />

        <include layout="@layout/item_line_vertical" />
    </LinearLayout>


</LinearLayout>
