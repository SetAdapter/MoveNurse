<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:background="#ff8193"
              android:orientation="horizontal">

    <TextView
        style="@style/recordTitle"
        android:layout_width="150dp"
        android:text="日期" />

    <include layout="@layout/item_line_vertical" />

    <TextView
        style="@style/recordTitle"
        android:layout_width="62dp"
        android:text="时间" />

    <include layout="@layout/item_line_vertical" />

    <TextView
        style="@style/recordTitle"
        android:layout_width="62dp"
        android:singleLine="false"
        android:text="T\n(℃)" />

    <include layout="@layout/item_line_vertical" />

    <TextView
        style="@style/recordTitle"
        android:layout_width="75dp"
        android:singleLine="false"
        android:text="p/HR\n(次/分)" />

    <include layout="@layout/item_line_vertical" />

    <TextView
        style="@style/recordTitle"
        android:layout_width="75dp"
        android:singleLine="false"
        android:text="R\n(次/分)" />

    <include layout="@layout/item_line_vertical" />

    <TextView
        style="@style/recordTitle"
        android:layout_width="75dp"
        android:singleLine="false"
        android:text="BP\n(mmHg)" />

    <include layout="@layout/item_line_vertical" />

    <TextView
        style="@style/recordTitle"
        android:layout_width="140dp"
        android:singleLine="false"
        android:text="SpO₂\n(%)" />

    <include layout="@layout/item_line_vertical" />

    <TextView
        style="@style/recordTitle"
        android:layout_width="140dp"
        android:singleLine="false"
        android:text="胎心\n次/分" />

    <include layout="@layout/item_line_vertical" />

    <TextView
        style="@style/recordTitle"
        android:layout_width="140dp"
        android:singleLine="false"
        android:text="自计胎动\n次/小时" />

    <include layout="@layout/item_line_vertical" />

    <TextView
        style="@style/recordTitle"
        android:layout_width="140dp"
        android:singleLine="false"
        android:text="胎位" />

    <include layout="@layout/item_line_vertical" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            style="@style/recordTitle"
            android:layout_width="270dp"
            android:text="宫缩" />

        <include layout="@layout/item_line" />

        <LinearLayout
            android:layout_width="270dp"
            android:layout_height="wrap_content">

            <TextView
                style="@style/recordTitle"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="强度" />

            <include layout="@layout/item_line_vertical" />

            <TextView
                style="@style/recordTitle"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:text="持续(S)/间歇(min)" />
        </LinearLayout>

    </LinearLayout>

    <include layout="@layout/item_line_vertical" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            style="@style/recordTitle"
            android:layout_width="200dp"
            android:text="宫颈" />

        <include layout="@layout/item_line" />

        <LinearLayout
            android:layout_width="200dp"
            android:layout_height="wrap_content">

            <TextView
                style="@style/recordTitle"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="扩张(cm)" />

            <include layout="@layout/item_line_vertical" />

            <TextView
                style="@style/recordTitle"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="质" />
        </LinearLayout>

    </LinearLayout>

    <include layout="@layout/item_line_vertical" />

    <TextView
        style="@style/recordTitle"
        android:layout_width="140dp"
        android:singleLine="false"
        android:text="先露高低\n(cm)" />

    <include layout="@layout/item_line_vertical" />

    <TextView
        style="@style/recordTitle"
        android:layout_width="140dp"
        android:text="胎膜" />

    <include layout="@layout/item_line_vertical" />

    <TextView
        style="@style/recordTitle"
        android:layout_width="140dp"
        android:text="羊水性状" />

    <include layout="@layout/item_line_vertical" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            style="@style/recordTitle"
            android:layout_width="200dp"
            android:text="入" />

        <include layout="@layout/item_line" />

        <LinearLayout
            android:layout_width="200dp"
            android:layout_height="wrap_content">

            <TextView
                style="@style/recordTitle"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="项目" />

            <include layout="@layout/item_line_vertical" />

            <TextView
                style="@style/recordTitle"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="量" />
        </LinearLayout>

    </LinearLayout>

    <include layout="@layout/item_line_vertical" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            style="@style/recordTitle"
            android:layout_width="200dp"
            android:text="出" />

        <include layout="@layout/item_line" />

        <LinearLayout
            android:layout_width="200dp"
            android:layout_height="wrap_content">

            <TextView
                style="@style/recordTitle"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="项目" />

            <include layout="@layout/item_line_vertical" />

            <TextView
                style="@style/recordTitle"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="量" />
        </LinearLayout>

    </LinearLayout>

    <include layout="@layout/item_line_vertical" />

    <TextView
        style="@style/recordTitle"
        android:layout_width="140dp"
        android:singleLine="false"
        android:text="氧气吸入\n(升/分)" />

    <include layout="@layout/item_line_vertical" />

    <TextView
        style="@style/recordTitle"
        android:layout_width="140dp"
        android:text="膝反射" />

    <include layout="@layout/item_line_vertical" />

    <TextView
        style="@style/recordTitle"
        android:layout_width="300dp"
        android:text="特殊情况记录" />

    <include layout="@layout/item_line_vertical" />

    <TextView
        style="@style/recordTitle"
        android:layout_width="125dp"
        android:text="护士签名" />

    <include layout="@layout/item_line_vertical" />

</LinearLayout>